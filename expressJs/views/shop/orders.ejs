<%- include('../includes/head.ejs') %>

</head>
<body>
    <%- include('../includes/navigation.ejs') %> 

    <main>
        <% if (orders.length <= 0) { %>
            <h1>Nothing Here</h1>
        <% } else { %>
            <ul>
                <% orders.forEach(order => { %>
                    <li>
                        <h1># <%= order._id %></h1>
                        <ul>
                            <% order.items.forEach(prod => { %>
                                <li>
                                    <h3><%= prod.title %> (<%= prod.quantity %>)</h3>
                                </li>
                            <% }) %>
                        </ul>
                    </li>
                <% }) %>
            </ul>
        <% } %>
    </main>

</body>

<%- include('../includes/end.ejs') %>
